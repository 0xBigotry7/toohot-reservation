{
  "name": "toohot-admin-dashboard-free",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "deploy": "vercel --prod",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "production-build": "NODE_ENV=production npm run type-check && npm run lint && npm run build",
    "health-check": "curl -f http://localhost:3002/api/health || exit 1",
    "db-indexes": "echo 'Run database/production_indexes.sql in Supabase SQL Editor'",
    "security-audit": "npm audit --audit-level moderate",
    "pre-deploy": "npm run security-audit && npm run production-build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "^2.38.4",
    "autoprefixer": "^10.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "nanoid": "^5.0.4",
    "next": "^14.2.30",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "resend": "^2.1.0",
    "stripe": "^18.4.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "typescript": "^5"
  }
}
